{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDb": {
    "ConnectionUri": "mongodb://localhost:27017",
    "DatabaseName": "WeatherDb",
    "Collections": {
      "Forecasts": "Forecasts"
    }
  },
  "OpenMeteoApi": {
    "BaseUrl": "https://api.open-meteo.com/v1/",
    "TimeoutInSeconds": 10
  },
  "GeocodingApi": {
    "BaseUrl": " https://geocode.maps.co",
    "ForwardEndpoint": "/search",
    "ReverseEndpoint": "/reverse",
    "TimeoutInSeconds": 10,
    "ApiKey": "InsertApiKey"
  }
}
